    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="TAILLE, i, puissance" type="Integer" array="False" size=""/>
            <assign variable="TAILLE" expression="10"/>
            <declare name="tab" type="Integer" array="True" size="TAILLE"/>
            <assign variable="puissance" expression="1"/>
            <assign variable="i" expression="0"/>
            <while expression="i &lt; TAILLE">
